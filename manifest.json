{
  "name": "Color Dev",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Customize the color scheme of the developers favorite pages",
  "homepage_url": "http://github.rodri",
  "icons": {
    "16": "icons/icon16.png",
    "72": "icons/icon72.png",
    "128": "icons/icon128.png"
  },
  "page_action" : {
      "default_icon" : "icons/stack19.png",
      "default_title" : "Choose your Color Scheme for stackoverflow",
      "default_popup": "src/page_action/page_action.html"
   },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://stackoverflow.com/*"
  ]
}